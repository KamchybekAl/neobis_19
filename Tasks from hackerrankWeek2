1.	SELECT * FROM CITY;
2.	SELECT * FROM CITY WHERE ID = 1661;
3.	SELECT * FROM CITY WHERE COUNTRYCODE = 'JPN';
4.	SELECT NAME FROM CITY WHERE COUNTRYCODE = 'JPN';
5.	SELECT CITY,STATE FROM STATION;
6.	SELECT DISTINCT CITY FROM STATION WHERE MOD(ID,2) = 0 ORDER BY CITY ASC;
7.	SELECT CITY, LENGTH(CITY) FROM STATION ORDER BY LENGTH(CITY) DESC,CITY ASC FETCH FIRST ROW ONLY;
8.	SELECT CITY, LENGTH(CITY) FROM STATION ORDER BY LENGTH(CITY) ASC,CITY ASC FETCH FIRST ROW ONLY;
9.	SELECT DISTINCT CITY FROM STATION WHERE CITY LIKE '%a' OR CITY LIKE '%e' OR CITY LIKE '%i' OR CITY LIKE '%o' OR CITY LIKE '%u';
10.	SELECT NAME FROM STUDENTS WHERE Marks > 75 ORDER BY SUBSTR(Name, LENGTH(Name)-2, 3), ID;
